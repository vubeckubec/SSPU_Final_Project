{block content}
    <table class="table table-striped">
    <div>playlists of user: {$usernameIs->username}</div>
    <thead>
        <th>Playlist number</th>
        <th>Playlist name</th>
    </thead>
    <tbody>
        {foreach $playlists_list as $playlist}
        <tr>
            <td>{$playlist->idplaylist}</td>
            <td>{$playlist->name}</td>
        </tr>
        {/foreach}
    </tbody>
</table>
{/block}
